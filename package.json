{
  "name": "rainlogger-back",
  "version": "1.0.1",
  "description": "Backend of rainlogger service",
  "main": "dist/server.js",
  "author": "macolmenerori",
  "repository": {
    "type": "git",
    "url": "https://github.com/macolmenerori/rainlogger-back.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "prettify": "prettier -w --log-level=silent src/",
    "lint": "eslint --fix .",
    "types": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "verify": "pnpm audit --prod && pnpm lint && pnpm prettify && pnpm types && pnpm build",
    "generate:openapi": "ts-node scripts/generateOpenapi.ts",
    "generate:openapi:json": "ts-node scripts/generateOpenapi.ts --json"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "dependencies": {
    "@exortek/express-mongo-sanitize": "^1.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "mongoose": "^9.2.1",
    "swagger-jsdoc": "^6.2.8",
    "yaml": "^2.8.2"
  },
  "pnpm": {
    "overrides": {
      "qs": "6.14.2"
    }
  }
}
